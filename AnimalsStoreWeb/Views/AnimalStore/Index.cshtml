<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="C:\Users\Рамазан\Desktop\сайты\ЗАКАЗНОЙ САЙТ\css\stylesforStore.css">
    <title>Document</title>
</head>
<body>
    <main>
        <h2 class="allservice">Список доступных животных</h2>
        <br>
        <br>

        <form id="filter" action="~/PostsStore/" class="filter">
            <select name="CityId" class="profile1">
                @if (Model.PaginationModel.CityId != CitiesHelper.ALL_CITIES)
                {
                    <option value="@CitiesHelper.ALL_CITIES">Все города</option>
                    foreach (City city in Model.SupportedCities)
                    {
                        if (city.Id == Model.PaginationModel.CityId)
                        {
                            <option class="variant" value="@city.Id" selected="selected">@city.CityName</option>
                        }
                        else
                        {
                            <option class="variant" value="@city.Id">@city.CityName</option>
                        }
                    }
                }
                else
                {
                    <option value="@CitiesHelper.ALL_CITIES" selected="selected">Все города</option>
                    foreach (City city in Model.SupportedCities)
                    {
                        <option class="variant" value="@city.Id" id="@city.Id">@city.CityName</option>
                    }
                }
            </select>
            <select name="ServiceId" class="profile2">
                @if (Model.PaginationModel.ServiceId != ServicesHelper.ALL_SERVICE)
                {
                    <option value="@ServicesHelper.ALL_SERVICE">Все услуги</option>
                    foreach (Service service in Model.SupportedServices)
                    {
                        if (service.Id == Model.PaginationModel.ServiceId)
                        {
                            <option class="variant" value="@service.Id" selected="selected">@service.ServiceName</option>
                        }
                        else
                        {
                            <option class="variant" value="@service.Id">@service.ServiceName</option>
                        }
                    }
                }
                else
                {
                    <option value="@ServicesHelper.ALL_SERVICE" selected="selected">Все услуги</option>
                    foreach (Service service in Model.SupportedServices)
                    {
                        <option class="variant" value="@service.Id">@service.ServiceName</option>
                    }
                }
            </select>
            <button name="UserAction" type="submit" value="new_filter" class="savebtn">Найти</button>
            <br>
            <br>
            @foreach (PostViewModel postModel in Model.Posts)
            {
                <br />
                <div class="serviceblock" onclick="location.href='/PostsStore/InfoAboutPost?id=@postModel.PostId'">
                    <img class="photo" width="20%" height="30%" src="data:image/jpeg;base64," alt="C:\Users\Рамазан\Desktop\сайты\ЗАКАЗНОЙ САЙТ\imges\cow.png">
                    <p class="service">Название</p>
                    <p class="price">Цена : @Math.Round(postModel.Price, 0) р</p>
                    <p class="animalinfo">Тип животного</p>
                    <p class="animalinfo">Описаниеdfgdfgggggggggggggggggggggggggg</p>
                    <p class="animalinfo">еще что-то</p>
                </div>
                <br />
            }

            <button name="UserAction" type="submit" value="back" class="savebtn">Назад</button>
            <button name="UserAction" type="submit" value="next" class="savebtn">Вперед</button>

        </form>
    </main>
</body>
</html>

